<div class="container mb-5 w-50 bg-test p-3 mt-5  ">
<h1 class="text-center">Creer votre note</h1>

<%= simple_form_for [@colocation, @note] do |f| %>


  <%= f.input :title, :label => "Titre de la note"  %>

  <%= f.input :content, :label => "Contenu de la note" %>





  <%= f.button :submit, "Ajouter la note", class: "btn btn-primary " %>

  <%= link_to "retour aux notes", colocation_notes_path, class: "btn btn-danger" %>
  <% end %>

</div>
